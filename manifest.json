{
    "manifest_version": 3,
    "name": "Organise Ideas",
    "version": "1.0",
    "description": "A simple to-do list extension",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_title": "To-Do List",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "permissions": [
      "activeTab",
      "storage"
    ]
  }
  